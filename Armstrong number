import java.io.*;
import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int num = sc.nextInt();

        int originalNum = num;
        int sum = 0;

        int temp = num;
        while (temp > 0) {
            sum += Math.pow(temp%10, 3);
            temp /= 10;
        }

        if (sum == originalNum) {
            System.out.println("Armstrong Number");
        } else {
            System.out.println("Not Armstrong Number");
        }
    }
}
