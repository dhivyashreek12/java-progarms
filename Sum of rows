package array2D_problems;

import java.util.Scanner;

public class SumOfRow {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int r = sc.nextInt();
		int c = sc.nextInt();
		int arr[][] = new int [r][c];
		//int sum = 0;
		for(int i = 0;i < r;i++) {
			for(int j = 0;j < c;j++) {
				arr[i][j] = sc.nextInt();
			}
		}
		for(int i = 0;i < r;i++) {
			int sum = 0;
			for(int j = 0;j < c;j++) {
				sum += arr[i][j];
			}
			System.out.println(sum);
		}
	}
}
