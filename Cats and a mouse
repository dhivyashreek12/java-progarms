import java.io.*;
import java.math.*;
import java.security.*;
import java.text.*;
import java.util.*;
import java.util.concurrent.*;
import java.util.regex.*;

public class CatsAndMouse {

    static String catAndMouse(int x, int y, int z) {
        int distA = Math.abs(x - z);  // distance between Cat A and Mouse
        int distB = Math.abs(y - z);  // distance between Cat B and Mouse

        if (distA < distB) {
            return "Cat A";
        } else if (distB < distA) {
            return "Cat B";
        } else {
            return "Mouse C";  // both reach at the same time
        }
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int q = sc.nextInt(); // number of queries
        for (int i = 0; i < q; i++) {
            int x = sc.nextInt();  // Cat A position
            int y = sc.nextInt();  // Cat B position
            int z = sc.nextInt();  // Mouse position

            System.out.println(catAndMouse(x, y, z));
        }

        sc.close();
    }
}
